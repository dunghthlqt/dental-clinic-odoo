<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Dental Patient Customer Code -->
        <record id="sequence_dental_patient" model="ir.sequence">
            <field name="name">Mã bệnh nhân</field>
            <field name="code">dental.patient</field>
            <field name="prefix">BN</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequence for Dental Treatment Medical Record Code -->
        <record id="sequence_dental_treatment" model="ir.sequence">
            <field name="name">Mã hồ sơ điều trị</field>
            <field name="code">dental.treatment</field>
            <field name="prefix">DT</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>

